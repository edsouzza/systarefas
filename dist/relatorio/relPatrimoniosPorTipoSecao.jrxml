	<queryString>
		<![CDATA[select p.codigo, p.serie, p.chapa, p.estacao, p.ip, s.nome as secao, c.nome as cliente
from
tblpatrimonios p, tblsecoes s, tblclientes c, tbltipos t
where
p.secaoid=s.codigo
and
p.clienteid=c.codigo
and
p.status='ATIVO'
and
p.tipoid=t.codigo
and
s.nome= $P{nomeSecao}
and
t.tipo= $P{tipoPatrimonio}]]>
	</queryString>
